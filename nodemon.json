{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "dist",
    "src/renderer/**/*"
  ],
  "verbose": true,
  "events": {
    "restart": "electron-rebuild"
  },
  "watch": [
    "src/main/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
} 